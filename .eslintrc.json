{
  "env": {
    "browser": true,
     "node": true,
     "es2021": true
   },
   "extends": [
     "eslint:recommended",
     "plugin:react/recommended",
     "plugin:react-hooks/recommended",
     "plugin:prettier/recommended"
   ],
   "plugins": ["react", "react-hooks", "prettier"],
   "settings": {
    "react": {
      "version": "detect"
    }
  },
   "parserOptions": {
     "ecmaVersion": 2021,
     "sourceType": "module",
     "ecmaFeatures": {
       "jsx": true
     }
   },
   "rules": {
   }
}
  